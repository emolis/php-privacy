{
    "name": "php-privacy/openpgp",
    "type": "library",
    "description": "PHP Privacy - The OpenPGP library in PHP language",
    "keywords": ["library", "openpgp", "end to end encryption", "privacy guard"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Nguyen Van Nguyen",
            "email": "nguyennv1981@gmail.com",
            "homepage": "https://github.com/nguyennv"
        }
    ],
    "require": {
        "php": ">=8.1",
        "phpseclib/phpseclib": "^3.0",
        "psr/log": "^3.0",
        "symfony/process": "^6.1 | ^7.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.0",
        "monolog/monolog": "^3.0",
        "phpunit/phpunit": "^9.0 | ^10.0"
    },
    "autoload": {
        "psr-4": {
            "OpenPGP\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OpenPGP\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit -c tests/phpunit.xml"
        ]
    }
}
