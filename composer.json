{
    "name": "php-pg/openpgp",
    "type": "library",
    "description": " PHP PG - The OpenPGP library in PHP language",
    "keywords": ["library", "OpenPGP", "end to end encryption", "PHP Privacy Guard"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Nguyen Van Nguyen",
            "email": "nguyennv1981@gmail.com",
            "homepage": "https://github.com/nguyennv"
        }
    ],
    "require": {
        "php": "^8.1",
        "phpseclib/phpseclib": "^3.0",
        "psr/log": "^3.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.0",
        "phpunit/phpunit": "^7.0 | ^8.0 | ^9.0",
        "fakerphp/faker": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "OpenPGP\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OpenPGP\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit",
            "vendor/bin/phpstan analyse"
        ]
    }
}
